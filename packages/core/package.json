{
  "name": "@mui-flexy/core",
  "version": "2.0.0",
  "description": "Core utilities and types for mui-flexy",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn dlx rollup -c rollup.config.mjs && yarn tsc -p tsconfig.dist.json",
    "build:watch": "nodemon --watch src --ext ts --exec 'yarn build' --delay 1000ms",
    "clean": "rm -rf dist && mkdir dist",
    "test": "cd ../.. && yarn test:core",
    "typecheck": "yarn tsc --noEmit"
  },
  "peerDependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@mui/material": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "@mui/system": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "react": "^17 || ^18 || ^19",
    "react-dom": "^17 || ^18 || ^19",
    "typescript": ">=5.4 || >=5.9.0-dev"
  },
  "devDependencies": {
    "@mui/material": "^7.0.0",
    "@mui/system": "^7.0.0",
    "@types/react": "^18.3.20",
    "chokidar-cli": "^3.0.0",
    "nodemon": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": ">=5.9.0-dev"
  }
}
