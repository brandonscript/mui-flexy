{
  "name": "@mui-flexy/v6",
  "version": "2.0.0",
  "description": "A flexbox convenience component for Material UI v6 Box and Grid with handy shorthand props",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn dlx rollup -c rollup.config.mjs && yarn tsc -p tsconfig.dist.json",
    "clean": "rm -rf dist && mkdir dist",
    "typecheck": "yarn tsc --noEmit"
  },
  "peerDependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@mui/material": "^6.0.0",
    "@mui/system": "^6.0.0",
    "react": "^17 || ^18 || ^19",
    "react-dom": "^17 || ^18 || ^19",
    "typescript": ">=5.4 || >=5.9.0-dev"
  },
  "dependencies": {
    "@mui-flexy/core": "workspace:*"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.0.0",
    "@mui/system": "^6.0.0",
    "@types/react": "^18.3.20",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.8.3"
  },
  "repository": "https://github.com/brandonscript/mui-flexy",
  "author": "github.com/brandonscript",
  "license": "MIT",
  "keywords": [
    "mui",
    "material-ui",
    "react",
    "flexbox",
    "flex",
    "layout",
    "grid",
    "box",
    "v6"
  ]
}
