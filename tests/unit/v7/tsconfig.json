{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "../../..",
    "rootDir": "../../..",
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@mui-flexy/core": [
        "packages/core/src"
      ],
      "@mui-flexy/v7": [
        "packages/v7/src"
      ],
      "@mui-flexy/v7/*": [
        "packages/v7/src/*"
      ],
      "@mui/material": [
        "./packages/v7/node_modules/@mui/material"
      ],
      "@mui/material/*": [
        "./packages/v7/node_modules/@mui/material/*"
      ],
      "@mui/system": [
        "./packages/v7/node_modules/@mui/system"
      ],
      "@mui/system/*": [
        "./packages/v7/node_modules/@mui/system/*"
      ],
      "@mui/styles": [
        "./packages/v7/node_modules/@mui/styles"
      ],
      "@mui/styles/*": [
        "./packages/v7/node_modules/@mui/styles/*"
      ],
      "@mui/utils": [
        "./packages/v7/node_modules/@mui/utils"
      ],
      "@mui/utils/*": [
        "./packages/v7/node_modules/@mui/utils/*"
      ],
      "@emotion/react": [
        "./packages/v7/node_modules/@emotion/react"
      ],
      "@emotion/styled": [
        "./packages/v7/node_modules/@emotion/styled"
      ]
    }
  },
  "include": [
    "./**/*",
    "../../../packages/v7/src/**/*"
  ]
}