{
  "name": "mui-flexy-demo",
  "version": "1.2.0",
  "description": "A little demo app for mui-flexy",
  "repository": "https://github.com/brandonscript/mui-flexy",
  "author": "github.com/brandonscript",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --hot --mode development",
    "build": "webpack --mode production",
    "lint": "eslint --ext .ts src",
    "dev:mui5": "yarn add @mui/material@^5 && yarn dev",
    "dev:mui6": "yarn add @mui/material@^6 && yarn dev",
    "dev:mui7": "yarn add @mui/material@^7 && yarn dev",
    "dev:mui-latest": "yarn add @mui/material@latest && yarn dev",
    "build:mui5": "yarn add @mui/material@^5 && yarn build",
    "build:mui6": "yarn add @mui/material@^6 && yarn build",
    "build:mui7": "yarn add @mui/material@^7 && yarn build",
    "build:mui-latest": "yarn add @mui/material@latest && yarn build"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7",
    "dev": "^0.1.3",
    "mui-flexy": "*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-syntax-highlighter": "^15.6.1",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@humanwhocodes/module-importer": "^1.0.1",
    "@types/react": "^18.3.19",
    "@types/react-dom": "^18.3.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "babel-loader": "^8.4.1",
    "css-loader": "^6.11.0",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^4.15.2"
  },
  "resolutions": {
    "mui-flexy": "portal:../"
  }
