{
  "name": "mui-flexy",
  "version": "2.0.2",
  "description": "A flexbox convenience component for Material UI Box and Grid with handy shorthand props",
  "type": "module",
  "workspaces": [
    "packages/*",
    "demos/*",
    "docs"
  ],
  "repository": "https://github.com/brandonscript/mui-flexy",
  "author": "github.com/brandonscript",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "yarn install && yarn clean && yarn build:packages && yarn build:docs",
    "build:all": "yarn build && yarn build:demos",
    "build:demos": "yarn workspaces foreach -p -A --include 'demos/*' run build",
    "build:docs": "cd docs && yarn build",
    "build:packages": "yarn workspace @mui-flexy/core run build && yarn workspaces foreach -p -A --include 'packages/v*' run build",
    "clean": "rm -rf packages/core/dist packages/v5/dist packages/v6/dist packages/v7/dist && mkdir -p packages/core/dist packages/v5/dist packages/v6/dist packages/v7/dist",
    "clean:all": "yarn clean && yarn workspaces foreach -A run clean",
    "demo:all": "concurrently \"yarn demo:v5\" \"yarn demo:v6\" \"yarn demo:v7\"",
    "demo:killall": "lsof -ti:3005,3006,3007 | xargs -r kill -15 || true; sleep 2; lsof -ti:3005,3006,3007 | xargs -r kill -9 || true",
    "demo:v5": "cd demos/v5 && yarn install && yarn dev",
    "demo:v6": "cd demos/v6 && yarn install && yarn dev",
    "demo:v7": "cd demos/v7 && yarn install && yarn dev",
    "docs:dev": "cd docs && yarn dev",
    "docs:dev:basepath": "cd docs && yarn dev:basepath",
    "docs:serve": "cd docs && yarn serve",
    "docs:serve:basepath": "cd docs && yarn serve:basepath",
    "docs:killall": "lsof -ti:3003 | xargs -r kill -15 || true; sleep 2; lsof -ti:3003 | xargs -r kill -9 || true",
    "lint": "prettier $(git diff --cached --name-only --diff-filter=ACMR | sed 's| |\\ |g') --write --ignore-unknown && eslint --config eslint.config.js --ext .ts,.tsx packages/*/src",
    "precommit": "./.husky/pre-commit",
    "publish": "./scripts/publish.sh",
    "publish:dry-run": "./scripts/publish.sh --dry-run",
    "test": "yarn test:unit && yarn test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:clean": "PLAYWRIGHT_FORCE_CLEAN=1 playwright test",
    "test:e2e:console": "playwright test tests/e2e/console-validation.spec.ts",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headless": "playwright test --headed=false",
    "test:e2e:setup": "playwright install",
    "test:e2e:report": "playwright show-report",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:demo": "playwright test tests/e2e/demo-common.spec.ts tests/e2e/demo-v5.spec.ts tests/e2e/demo-v6.spec.ts tests/e2e/demo-v7.spec.ts tests/e2e/console-validation.spec.ts",
    "test:e2e:v5": "playwright test tests/e2e/demo-v5.spec.ts",
    "test:e2e:v6": "playwright test tests/e2e/demo-v6.spec.ts",
    "test:e2e:v7": "playwright test tests/e2e/demo-v7.spec.ts",
    "test:e2e:docs": "playwright test --config=playwright.docs.config.ts",
    "test:unit": "yarn test:unit:core && yarn test:unit:v5 && yarn test:unit:v6 && yarn test:unit:v7",
    "test:unit:core": "yarn jest --selectProjects=core-tests",
    "test:unit:v5": "yarn jest --selectProjects=v5-tests",
    "test:unit:v6": "yarn jest --selectProjects=v6-tests",
    "test:unit:v7": "yarn jest --selectProjects=v7-tests",
    "typecheck": "yarn workspaces foreach -p -A run typecheck",
    "set-version": "./scripts/version.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/jest": "^11.13.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.24.0",
    "@humanwhocodes/module-importer": "^1.0.1",
    "@playwright/test": "^1.53.2",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.18",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/babel__core": "^7.20.5",
    "@types/jest": ">=30.0.0-alpha.7",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/rollup-plugin-peer-deps-external": "^2.2.5",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/parser": "^8.29.1",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.24.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import-x": "^4.10.2",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jest": ">=30.0.0-alpha.7",
    "jest-environment-jsdom": "^29.7.0",
    "pinst": "^3.0.0",
    "playwright": "^1.53.2",
    "prettier": "3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.39.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "stable-hash": ">=0.0.5",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "webpack": "^5.99.5"
  },
  "packageManager": "yarn@4.9.2",
  "npm.packageManager": "yarn@4.9.2"
}
