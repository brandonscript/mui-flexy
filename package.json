{
  "name": "mui-flexy",
  "version": "1.2.4",
  "description": "A flexbox convenience component for Material UI Box and Grid with handy shorthand props",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/brandonscript/mui-flexy",
  "author": "github.com/brandonscript",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "clean:docs": "rm -rf docs/static && mkdir -p docs/static",
    "build": "yarn install && yarn clean && rollup -c rollup.config.mjs && tsc -p tsconfig.dist.json",
    "build:docs": "yarn install && yarn clean:docs && rollup -c rollup.docs.mjs",
    "build:all": "yarn build && yarn build:docs",
    "docs": "cd docs && npx http-server -o ./",
    "ship": "./ship.sh",
    "bump": "./up.sh",
    "lint": "prettier $(git diff --cached --name-only --diff-filter=ACMR | sed 's| |\\ |g') --write --ignore-unknown && eslint --config eslint.config.js --ext .ts,.tsx src",
    "test": "NODE_OPTIONS=--experimental-vm-modules ./node_modules/.bin/jest --config jest.config.js",
    "typecheck": "npx tsc --project tsconfig.json --noEmit",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@mui/material": "^5 || ^6",
    "react": "^17 || ^18 || ^19",
    "react-dom": "^17 || ^18 || ^19",
    "typescript": ">=5.4 || >=5.9.0-dev"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/jest": "^11.13.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.24.0",
    "@humanwhocodes/module-importer": "^1.0.1",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.18",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^28.1.8",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/rollup-plugin-peer-deps-external": "^2.2.5",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/parser": "^8.29.1",
    "babel-jest": "^29.7.0",
    "eslint": "^9.24.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import-x": "^4.10.2",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jest": ">=30.0.0-alpha.7",
    "jest-environment-jsdom": "^29.7.0",
    "pinst": "^3.0.0",
    "prettier": "3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.39.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "stable-hash": ">=0.0.5",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "webpack": "^5.99.5"
  }
}
