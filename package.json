{
  "name": "mui-flexy",
  "version": "1.2.4",
  "description": "A flexbox convenience component for Material UI Box and Grid with handy shorthand props",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*",
    "demos/*"
  ],
  "repository": "https://github.com/brandonscript/mui-flexy",
  "author": "github.com/brandonscript",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "clean": "rm -rf packages/*/dist && find packages -name dist -type d -exec mkdir -p {} \\;",
    "clean:docs": "rm -rf docs/static && mkdir -p docs/static",
    "build": "yarn install && yarn clean && yarn workspaces foreach -A run build",
    "build:docs": "yarn install && yarn clean:docs && rollup -c rollup.docs.mjs",
    "build:all": "yarn build && yarn build:docs",
    "docs": "cd docs && npx http-server -o ./",
    "ship": "./ship.sh",
    "bump": "./up.sh",
    "lint": "prettier $(git diff --cached --name-only --diff-filter=ACMR | sed 's| |\\ |g') --write --ignore-unknown && eslint --config eslint.config.js --ext .ts,.tsx packages/*/src",
    "demo": "cd demo && npm run dev",
    "test": "yarn workspaces foreach -A run test",
    "test:v5": "yarn workspace mui-flexy-v5 test",
    "test:v6": "yarn workspace mui-flexy-v6 test",
    "test:v7": "yarn workspace mui-flexy-v7 test",
    "test:core": "yarn workspace @mui-flexy/core test",
    "typecheck": "yarn workspaces foreach -A run typecheck",
    "typecheck:v5": "yarn workspace mui-flexy-v5 typecheck",
    "typecheck:v6": "yarn workspace mui-flexy-v6 typecheck",
    "typecheck:v7": "yarn workspace mui-flexy-v7 typecheck",
    "typecheck:core": "yarn workspace @mui-flexy/core typecheck",
    "test:e2e": "playwright test",
    "test:e2e:v5": "playwright test tests/e2e/demo-v5.spec.ts",
    "test:e2e:v6": "playwright test tests/e2e/demo-v6.spec.ts",
    "test:e2e:v7": "playwright test tests/e2e/demo-v7.spec.ts",
    "test:e2e:comparison": "playwright test tests/e2e/demo-comparison.spec.ts",
    "test:e2e:console": "playwright test tests/e2e/console-validation.spec.ts",
    "test:e2e:headless": "playwright test --headed=false",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/jest": "^11.13.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.24.0",
    "@humanwhocodes/module-importer": "^1.0.1",
    "@playwright/test": "^1.53.2",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.18",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^28.1.8",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/rollup-plugin-peer-deps-external": "^2.2.5",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/parser": "^8.29.1",
    "babel-jest": "^29.7.0",
    "eslint": "^9.24.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import-x": "^4.10.2",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jest": ">=30.0.0-alpha.7",
    "jest-environment-jsdom": "^29.7.0",
    "pinst": "^3.0.0",
    "playwright": "^1.53.2",
    "prettier": "3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.39.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "stable-hash": ">=0.0.5",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "webpack": "^5.99.5"
  }
}
